# Uninstall desired apps
brew cask zap caffeine
brew cask zap gitx
brew cask zap java

brew cask zap justlooking

# Uninstall these apps and corresponding files

brew cask zap alfred
rm -rf ~/Library/Application\ Support/Alfred\ 2/

brew cask zap ccleaner
rm -rf ~/Library/Application\ Support/CCleaner/

brew cask zap firefox
rm -rf ~/Library/Application\ Support/Firefox/
rm -rf ~/Library/Application\ Support/Mozilla/

brew cask zap flux
rm -rf ~/Library/Application\ Support/Flux/

brew cask zap github
rm -rf ~/Library/Application\ Support/GitHub\ for\ Mac/

brew cask zap google-chrome
rm -rf /Library/Application\ Support/Google/
rm -rf /Library/Google/

brew cask zap iterm2
rm -rf ~/Library/Application\ Support/iTerm

brew cask zap nvalt
rm -rf ~/Library/Application\ Support/Notational\ Data/
rm -rf ~/Library/Application\ Support/nvALT

brew cask zap sublime-text
rm -rf ~/Library/Application\ Support/Sublime\ Text\ 2/

brew cask zap virtualbox
rm -rf ~/VirtualBox\ VMs/
rm -rf /Library/Application\ Support/VirtualBox/


